#test determinant_1
matrix_t A;
double det = 0;
s21_create_matrix(2, 3, &A);
int err = s21_determinant(&A, &det);
ck_assert_int_eq(err, CALCULATION_ERROR);
s21_remove_matrix(&A);

#test determinant_2
matrix_t A;
double det = 0;
s21_create_matrix(1, 1, &A);
A.matrix[0][0] = 11;
int err = s21_determinant(&A, &det);
ck_assert_int_eq(err, OK);
ck_assert_double_eq(det, 11);
s21_remove_matrix(&A);

#test determinant_3
matrix_t A;
double det = 0;
s21_create_matrix(2, 2, &A);
A.matrix[0][0] = 11;
A.matrix[0][1] = -3;
A.matrix[1][0] = -15;
A.matrix[1][1] = -2;
int err = s21_determinant(&A, &det);
ck_assert_int_eq(err, OK);
ck_assert_double_eq(det, -67);
s21_remove_matrix(&A);

#test determinant_4
matrix_t A;
double det = 0;
s21_create_matrix(3, 3, &A);
A.matrix[0][0] = 1;
A.matrix[0][1] = -2;
A.matrix[0][2] = 3;
A.matrix[1][0] = 4;
A.matrix[1][1] = 0;
A.matrix[1][2] = 6;
A.matrix[2][0] = -7;
A.matrix[2][1] = 8;
A.matrix[2][2] = 9;
int err = s21_determinant(&A, &det);
ck_assert_int_eq(err, OK);
ck_assert_double_eq(det, 204);
s21_remove_matrix(&A);

#test determinant_5
matrix_t A;
double det = 0;
s21_create_matrix(4, 4, &A);
A.matrix[0][0] = 3;
A.matrix[0][1] = -3;
A.matrix[0][2] = -5;
A.matrix[0][3] = 8;
A.matrix[1][0] = -3;
A.matrix[1][1] = 2;
A.matrix[1][2] = 4;
A.matrix[1][3] = -6;
A.matrix[2][0] = 2;
A.matrix[2][1] = -5;
A.matrix[2][2] = -7;
A.matrix[2][3] = 5;
A.matrix[3][0] = -4;
A.matrix[3][1] = 3;
A.matrix[3][2] = 5;
A.matrix[3][3] = -6;
int err = s21_determinant(&A, &det);
ck_assert_int_eq(err, OK);
ck_assert_double_eq(det, 18);
s21_remove_matrix(&A);
